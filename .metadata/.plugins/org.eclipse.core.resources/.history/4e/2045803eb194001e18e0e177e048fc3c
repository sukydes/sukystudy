package base_java;

import java.util.jar.Attributes.Name;

public class PersonInfo {
	String name ="";
	int age = 0;
	String gender = "";
	String address = "";
	String desc = "";
	
	public PersonInfo(String name, int age, String gender, String address, String desc) {
		this.name=name;
		this.age=age;
		this.gender=gender;
		this.address=address;
		this.desc=desc;
		System.out.println(name + "/" + age + "/" + gender + "/" + address + "/" + desc);
	}
	public PersonInfo(String name, int age) {
		this.name=name;
		this.age=age;
		System.out.println(name + "/" + age);
	}
	public PersonInfo(String gender, String address, String desc) {
		this.gender=gender;
		this.address=address;
		this.desc=desc;

		System.out.println(gender + "/" + address + "/" + desc);
	}
	
	
	
}
